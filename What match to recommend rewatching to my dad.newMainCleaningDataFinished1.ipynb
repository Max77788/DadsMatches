  {
   "cell_type": "code",
   "execution_count": 30,
   "id": "1fdba86d",
   "metadata": {},
   "outputs": [],
   "source": [
    "#Calculate the amount of matches every team participated in\n",
    "FC_dict = {}\n",
    "for x in all_uniq_teams:\n",
    "    i=0\n",
